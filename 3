#include <iostream>

using namespace std;

int main()
{
    int i, n, max, min;
    cin>>n;
    int a[n];
    max=-1;
    min=1000000;
    for (i=0; i<n; i++)
    {
        cin>>a[i];
    }
    for (i=0; i<n; i++)
    {
        if (a[i]>max)
        {
            max=a[i];
        }
    }
    for (i=0; i<n; i++)
    {
        if (a[i]<min)
        {
            min=a[i];
        }
    }
    for (i=0; i<n; i++)
    {
        if (a[i]==max)
        {
            cout<<a[i]<<" "<<"0"<<" ";
        } else if (a[i]==min)
        {
            cout<<"0"<<" "<<a[i]<<" ";
        } else if (a[i]!=min && a[i]!=max)
        {
            cout<<a[i]<<" ";
        }
    }
    return 0;
}
