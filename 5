#include <iostream>

using namespace std;

int main()
{
    int i, n;
    cin>>n;
    int a[n];
    for (i=0; i<n; i++)
    {
        cin>>a[i];
    }
    for (i=0; i<n; i++)
    {
        if (a[i]>0)
        {
            cout<<"0"<<" "<<a[i]<<" ";
        } else
        {
            cout<<a[i]<<" ";
        }
    }
    
    return 0;
}
